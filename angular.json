{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "angular-slack",
  "projects": {
    "angular-slack": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "changeDetection": "OnPush"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "apps/angular-slack",
      "sourceRoot": "apps/angular-slack/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/angular-slack",
            "index": "apps/angular-slack/src/index.html",
            "main": "apps/angular-slack/src/main.ts",
            "polyfills": "apps/angular-slack/src/polyfills.ts",
            "tsConfig": "apps/angular-slack/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/angular-slack/src/favicon.ico",
              "apps/angular-slack/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "./node_modules/font-awesome/css/font-awesome.css",
              "./node_modules/@ctrl/ngx-emoji-mart/picker.css",
              "apps/angular-slack/src/styles.scss"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": ["md5", "socket.io-client"],
            "stylePreprocessorOptions": {
              "includePaths": ["apps/angular-slack/src/assets/scss"]
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/angular-slack/src/environments/environment.ts",
                  "with": "apps/angular-slack/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular-slack:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular-slack:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-slack:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "codeCoverage": true,
            "main": "apps/angular-slack/src/test.ts",
            "polyfills": "apps/angular-slack/src/polyfills.ts",
            "tsConfig": "apps/angular-slack/tsconfig.spec.json",
            "karmaConfig": "apps/angular-slack/karma.conf.js",
            "assets": [
              "apps/angular-slack/src/favicon.ico",
              "apps/angular-slack/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "./node_modules/font-awesome/css/font-awesome.css",
              "./node_modules/@ctrl/ngx-emoji-mart/picker.css",
              "apps/angular-slack/src/styles.scss"
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": ["apps/angular-slack/src/assets/scss"]
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/angular-slack/tsconfig.app.json",
              "apps/angular-slack/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "angular-slack:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular-slack:serve:production"
            }
          }
        }
      }
    },
    "angular-slack-e2e": {
      "root": "apps/angular-slack-e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/angular-slack-e2e/protractor.conf.js",
            "devServerTarget": "angular-slack:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular-slack:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/angular-slack-e2e/tsconfig.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
